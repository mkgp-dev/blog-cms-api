{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "functions": {
        "api/index.ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/health"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        }
    ]
}